<?xml version="1.0"?>
<component name="fr.toutatice.ecm.platform.core.file.converter">

	<require>org.nuxeo.ecm.platform.convert.plugins</require>

	<extension target="org.nuxeo.ecm.core.convert.service.ConversionServiceImpl"
		point="converter">
        
        <!-- New source mime-type -->
		<converter name="any2pdf">
			<sourceMimeType>application/x-extension-conf</sourceMimeType>
			<sourceMimeType>application/x-extension-properties</sourceMimeType>
			<sourceMimeType>text/x-java</sourceMimeType>
			<sourceMimeType>text/x-ldif</sourceMimeType>
		</converter>

	</extension>

</component>