opentoutatice.target=.
nuxeo.template.includes=postgresql

##----------------------------------------------------------------------------
## Permalink service
##----------------------------------------------------------------------------
nuxeo.permalink.service.portal.url=localhost
nuxeo.permalink.service.param1=portail
nuxeo.permalink.service.param2=cms

##----------------------------------------------------------------------------
## JSF States stack
##----------------------------------------------------------------------------
nuxeo.jsf.numberOfViewsInSession=20
nuxeo.jsf.numberOfLogicalViews=20

##----------------------------------------------------------------------------
## PDF conversion: 
##----------------------------------------------------------------------------
# soffice user environments pool size
ottc.converter.toutaticeAny2pdf.poolSize=10
# conversion timeout
# time to wait for the conversion before timeout, in seconds
ottc.converter.toutaticeAny2pdf.timeoutDuration=30

##========================
## Fulltext Analyzer
##========================
nuxeo.vcs.fulltext.search.analyzer=french

##-------------------------
## Timeout for imports
##-------------------------
org.nuxeo.dnd.upload.timeout=300000
org.nuxeo.dnd.exec.timeout=300000
nuxeo.db.transactiontimeout=700

##----------------------------------------------------------------------------
## currentlyEditedCache
##----------------------------------------------------------------------------
# duration a cache entry is kept, in minute
ottc.currentlyEditedCache.ttl=5
# maximum number of entries the cache may contain
ottc.currentlyEditedCache.maxsize=1000

##----------------------------------------------------------------------------
## recentlyEditedCache
##----------------------------------------------------------------------------
# duration a cache entry is kept, in minute
ottc.recentlyEditedCache.ttl=5
# maximum number of entries the cache may contain
ottc.recentlyEditedCache.maxsize=1000