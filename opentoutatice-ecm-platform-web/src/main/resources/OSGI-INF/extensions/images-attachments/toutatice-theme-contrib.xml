<?xml version="1.0"?>
<component name="fr.toutatice.ecm.platform.web.images.theme">

	<require>org.nuxeo.theme.services.ThemeService</require>
	<require>org.nuxeo.theme.styling.service</require>
	
	<extension target="org.nuxeo.theme.services.ThemeService" point="applications">
	
		<application root="${org.nuxeo.ecm.contextPath}" template-engine="jsf-facelets">
			<negotiation>
				<strategy>nuxeo5</strategy>
				<default-engine>default</default-engine>
				<default-theme>galaxy/default</default-theme>
				<default-perspective>default</default-perspective>
			</negotiation>
			<view id="/acaren_create_image_search_document_popup.xhtml">
				<theme>galaxy/popup</theme>
			</view>
<!-- 			<view id="/acaren_create_relation_search_document_popup.xhtml">
				<theme>toutatice/popup</theme>
			</view> -->
		</application>
		
	</extension>  	
	
</component>